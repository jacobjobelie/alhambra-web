  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "scripts": {
    "test": "karma start",
    "check-coverage": "istanbul check-coverage --statements 23 --branches 5 --functions 9 --lines 24",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel validate-webpack:* lint test --serial check-coverage",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "clean-dist": "rimraf dist && mkdir dist && mkdir dist/js",
    "copy-files": "cpy src/index.html dist",
    "clean-and-copy": "npm run copy-files",
    "prestart": "npm run clean-and-copy",
    "start": "NODE_ENV=development webpack-dev-server --env.dev --content-base dist",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build": "NODE_ENV=development webpack --env.dev",
    "build:prod": "NODE_ENV=production webpack --env.prod",
    "lint": "eslint .",
    "setup": "npm install && npm run validate"
  }
}
